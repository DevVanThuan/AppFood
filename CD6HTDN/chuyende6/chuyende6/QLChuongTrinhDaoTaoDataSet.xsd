                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>WITH TongSoTheoLoai AS (
    SELECT LoaiMon,
           SUM(MonHoc.SoTinChi) AS TotalSoTinChi,
           SUM(MonHoc.LyThuyet + MonHoc.ThucHanh + MonHoc.KiemTra ) AS Totalgiotheoloai,
           SUM(MonHoc.LyThuyet) AS Totallythuyettheoloai,
           SUM(MonHoc.ThucHanh) AS Totalthuchanhtheoloai,
           SUM(MonHoc.KiemTra) AS TotalKiemTratheoloai
    FROM ChiTietCTDT
    INNER JOIN MonHoc ON ChiTietCTDT.MaMH = MonHoc.MaMH
    WHERE ChiTietCTDT.MaCTDT = @MaCTDT
      AND ChiTietCTDT.MaNghe = @MaNghe
    GROUP BY LoaiMon
)
SELECT ChiTietCTDT.MaCTDT, 
       ChiTietCTDT.MaMH, 
       ChiTietCTDT.MaNghe, 
       ChiTietCTDT.HocKy,
       MonHoc.TenMH, 
       MonHoc.SoTinChi, 
       MonHoc.LyThuyet, 
       MonHoc.ThucHanh, 
       MonHoc.KiemTra, 
       MonHoc.ThuongXuyen, 
       MonHoc.DinhKy, 
       MonHoc.HetMon, 
       MonHoc.GhiChu,
       ChiTietCTDT.LoaiMon,
       TongSoTheoLoai.TotalSoTinChi,
       TongSoTheoLoai.Totalgiotheoloai,
       TongSoTheoLoai.Totallythuyettheoloai,
       TongSoTheoLoai.Totalthuchanhtheoloai,
       TongSoTheoLoai. TotalKiemTratheoloai,
       (MonHoc.LyThuyet + MonHoc.ThucHanh + MonHoc.KiemTra ) AS Totalgiotheomon
FROM ChiTietCTDT
INNER JOIN MonHoc ON ChiTietCTDT.MaMH = MonHoc.MaMH
INNER JOIN TongSoTheoLoai ON ChiTietCTDT.LoaiMon = TongSoTheoLoai.LoaiMon
WHERE ChiTietCTDT.MaCTDT = @MaCTDT
  AND ChiTietCTDT.MaNghe = @MaNghe
ORDER BY
  CASE
    WHEN ChiTietCTDT.LoaiMon = 'Môn Chung' THEN 1
    WHEN ChiTietCTDT.LoaiMon = 'Môn Học Cơ Sở' THEN 2
    WHEN ChiTietCTDT.LoaiMon = 'Môn Học Chuyên Môn Nghề' THEN 3
    WHEN ChiTietCTDT.LoaiMon = 'Môn Học Chuyên Môn Nghề Tự Chọn' THEN 4
  END,
  ChiTietCTDT.LoaiMon;
</CommandText>
